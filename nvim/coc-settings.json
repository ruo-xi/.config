{
    "languageserver": {
        "clangd": {
            "command": "clangd",
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json"
            ],
            "filetypes": [
                "c",
                "cc",
                "cpp",
                "c++",
                "objc",
                "objcpp"
            ]
        }
    },
    "coc.preferences.colorSupport": true,
    "explorer.icon.enableNerdfont": true
}

